{
    "configurations": [
        {
            "type": "java",
            "name": "scriptParser",
            "request": "launch",
            "mainClass": "com.sekhanov.scriptparser.App",
            "args": "sort.script",
            "stepFilters": {
                "classNameFilters": [
                    "java.*",
                    "javax.*",
                    "com.sun.*",
                    "sun.*",
                    "sunw.*",
                    "org.omg.*"
                ],
                "skipSynthetics": false,
                "skipStaticInitializers": false,
                "skipConstructors": false
            }
        }
    ]
}